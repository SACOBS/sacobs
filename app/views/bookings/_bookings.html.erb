<%= paginate bookings, param_name: "#{type}_page", remote: true %>
<% if bookings.any? %>
    <table class="table table-hover table-condensed sortable">
      <thead>
      <tr>
        <th>Actions</th>
        <th>Booked</th>
        <th>Client</th>
        <th>Contact</th>
        <th>Trip Date</th>
        <th>Qty</th>
        <th>Connection</th>
      </tr>
      </thead>
      <tbody>
      <%= render bookings, cache: true %>
      </tbody>
    </table>
<% else %>
    <h3>There are currently 0 <%= type %> bookings.</h3>
<% end %>
