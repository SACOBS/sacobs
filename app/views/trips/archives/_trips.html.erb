<%= paginate trips %>
<table class='table table-hover table-condensed sortable'>
  <thead>
    <tr>
      <th>Actions</th>
      <th>Name</th>
      <th>Day of Week</th>
      <th>Start Date</th>
      <th>End Date</th>
      <th>Route</th>
      <th>Bus</th>
    </tr>
  </thead>
  <tbody>
    <%= render partial: 'trip', collection: trips, as: :trip, cache: true %>
  </tbody>
</table>
<%= paginate trips %>
